DROP TABLE DBA_FIND.PROC_ERROR CASCADE CONSTRAINTS;

CREATE TABLE DBA_FIND.PROC_ERROR
(
  OP_TYPE        VARCHAR2(50 BYTE),
  TABLE_NAME     VARCHAR2(50 BYTE),
  ERROR_MESSAGE  VARCHAR2(2000 BYTE),
  ERR_DATE       DATE                           DEFAULT sysdate,
  SQL_TEXT       VARCHAR2(2000 BYTE)
)
TABLESPACE USERS
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE;
