DROP TABLE DBA_FIND.AUDIT_LOGON CASCADE CONSTRAINTS;

CREATE TABLE DBA_FIND.AUDIT_LOGON
(
  LOGON_TIME  DATE,
  SESSION_ID  NUMBER,
  OS_USER     VARCHAR2(200 BYTE),
  IP_ADDRESS  VARCHAR2(200 BYTE),
  TERMINAL    VARCHAR2(200 BYTE),
  HOST        VARCHAR2(200 BYTE),
  USER_NAME   VARCHAR2(30 BYTE)
)
TABLESPACE USERS
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE;
